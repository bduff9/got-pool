    "meteor-node-stubs": "0.2.11",
    "prop-types": "15.6.2",
    "react": "15.7.0",
    "react-dom": "15.6.2",
    "react-helmet": "5.2.0",
    "react-infinite-scroller": "1.2.0",
    "react-komposer": "1.13.1",
    "react-router-dom": "4.3.1",
    "yup": "0.26.5"
  },
  "devDependencies": {
    "autoprefixer": "9.1.5",
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "jshint": "2.9.6"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
